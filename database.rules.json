{
  "rules": {
    "flamelink": {
      ".read": true,
      ".write": "auth != null",
      "users": {
        ".indexOn": ["email"]
      },
      "environments": {
        "production": {
          "content": {
            "article": {
              "$en-US": {
                ".indexOn": ["status"]
              }
            }
          }
        }
      }
    }
  }
}